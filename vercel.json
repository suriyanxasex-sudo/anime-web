{
  "version": 2,
  "crons": [
    {
      "path": "/api/cron/auto?key=joshua7465",
      "schedule": "0 */6 * * *",
      "description": "Jplus_Auto_Sync_Bot: รันทุก 6 ชั่วโมงเพื่อขุดมังงะตอนใหม่"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "/api/admin/:path*"
    }
  ]
}